Algoritmo "Tarefa 1"

//Variaveis
Var
   x, y, soma : inteiro

   // Inicio do algoritimo
Inicio

   Escreva ( "Insira o valor: " )
   leia ( x )
   EscrevaL
   
   Escreva ( "Insira o valor: " )
   leia ( y )
   EscrevaL
   soma <- ( 37 * x ) + ( 42 * y )

   Escreva( "O resultado final é: ", soma )

Fimalgoritmo